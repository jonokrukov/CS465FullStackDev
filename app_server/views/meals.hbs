<!DOCTYPE html>
<!-- Website template by freewebsitetemplates.com -->
<html>
<head>
	<meta charset="UTF-8">
	<title>Foods - Travlr Getaways Website Template</title>
	<link rel="stylesheet" href="css/style.css" type="text/css">
</head>
<body>
	<div id="background">
		<div id="page">
			{{> header_meal}}
			<div id="contents">
				<div class="box">
					<div>
						<div class="body">
							<h1>Meals</h1>
							<ul id="foods">
			{{#each food_choices}}
								<li>
									<a href="/meals"><img src="images/{{this.image}}" alt="Img"</a>
									<h2><a href="/meals">{{this.name}}</a></h2>
									{{{this.description}}}
								</li>
			{{/each}}
							</ul>
						</div>
					</div>
				</div>
			</div>
		</div>
		{{> footer_meal}}
	</div>
</body>
</html>